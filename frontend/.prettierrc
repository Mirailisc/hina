{
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "endOfLine": "auto",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/pages/(.*)$",
    "^@/lib/(.*)$",
    "^@/context/(.*)$",
    "^@/hooks/(.*)$",
    "^@/constants/(.*)$",
    "^@/images/(.*)$",
    "^@/gql/(.*)$",
    "^@(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
